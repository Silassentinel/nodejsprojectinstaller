{
  "env": {
    "browser": true,
    "es2021": true,
    "es6": true
  },
  "plugins": ["security","@babel","prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:security/recommended",
    "airbnb-base",
    "prettier"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
        "restParams": true,
        "modules": true
    }
  },
  "rules": {
    "max-len": ["error", {"code": 120}],
    "no-useless-constructor":0,
    "no-unused-vars": 2,
    "for-direction": 2,
    "function-paren-newline": ["error", "multiline-arguments"],
    "semi": "error",
    "no-multi-spaces": ["error"],
    "no-tabs": [2, {"allowIndentationTabs": true}],
    "import/extensions": [2, "never"],
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true
      }
    ],
    "arrow-parens": [2, "always"],
    "constructor-super": 2,
    "no-const-assign": 2,
    "no-this-before-super": 2,
    "no-var": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prettier/prettier": 2
  }
}
